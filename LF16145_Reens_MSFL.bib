journal = {New Journal of Physics},
keywords = {Stark effect,Zeeman effect,cold molecules,free radicals,hydroxyl,molecular spectroscopy},
number = {4},
pages = {45014},
publisher = {IOP Publishing},
title = {{Hyperfine structure of the hydroxyl free radical (OH) in electric and magnetic fields}},
url = {http://dx.doi.org/10.1088/1367-2630/17/4/045014},
volume = {17},
year = {2015}
}
