file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Moses et al. - 2015 - Creation of a low-entropy quantum gas of polar molecules in an optical lattice.pdf:pdf;:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Moses et al. - 2015 - Creation of a low-entropy quantum gas of polar molecules in an optical lattice(2).pdf:pdf},
issn = {0036-8075},
journal = {Science},
month = {nov},
number = {6261},
