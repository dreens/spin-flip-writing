@article{Bergeman1989,
abstract = {We consider quantized motion of neutral atoms cooled below the recoil limit in a quadrupole magnetostatic trap. Because of Majorana transitions to untrapped levels near the point of zero field at the trap center, all quantum levels have a nonzero decay rate. The Schr{\"{o}}dinger equation associated with the potential g$\mu$B {\textperiodcentered} S (S is the total atomic spin) takes the form of coupled equations in r when the spinor components are expanded in spherical harmonics. We integrate the multichannel problem numerically to obtain asymptotic phase shifts, resonance energies, and widths. For S = 1/2, the lowest levels have widths somewhat less than their spacing. Thus the trap quantum-level structure might possibly be observable if the atoms are sufficiently cold, namely, in the 0.1-$\mu$K regime for most atoms and attainable trap field gradients. The width decreases rapidly with increasing MJ, the angular momentum about the symmetry axis. Spectroscopic linewidths of a few hertz are possible if there is enough population in the lowest levels with a few MJ quanta. The decay rate of the lowest levels, however, is probably too rapid for studying Bose–Einstein condensation in such a trap.},
author = {Bergeman, T. H. and McNicholl, Patrick and Kycia, Jan and Metcalf, Harold and Balazs, N. L.},
doi = {10.1364/JOSAB.6.002249},
file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Bergeman et al. - 1989 - Quantized motion of atoms in a quadrupole magnetostatic trap.pdf:pdf},
issn = {0740-3224},
journal = {Journal of the Optical Society of America B},
number = {11},
pages = {2249},
title = {{Quantized motion of atoms in a quadrupole magnetostatic trap}},
url = {http://josab.osa.org/abstract.cfm?URI=josab-6-11-2249},
volume = {6},
year = {1989}
}
