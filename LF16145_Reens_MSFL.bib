number = {12},
pages = {1620--1635},
title = {{Long-Lived Ultracold Molecules with Electric and Magnetic Dipole Moments}},
url = {http://arxiv.org/abs/1707.03925},
volume = {3},
year = {2017}
}
@article{Segev2017,
abstract = {Supersonic beams are a prevalent source of cold molecules utilized in the study of chemical reactions, atom interferometry, gas-surface interactions, precision spectroscopy, molecular cooling and more. The triumph of this method emanates from the high densities produced in relation to other methods, however beam density remains fundamentally limited by interference with shock waves reflected from collimating surfaces. Here we show experimentally that this shock interaction can be reduced or even eliminated by cryo-cooling the interacting surface. An increase in beam density of nearly an order of magnitude was measured at the lowest surface temperature, with no further fundamental limitation reached. Visualization of the shock waves by plasma discharge and reproduction with direct simulation Monte Carlo calculations both indicate that the suppression of the shock structure is partially caused by lowering the momentum flux of reflected particles, and significantly enhanced by the adsorption of particles to the surface. We observe that the scaling of beam density with source pressure is recovered, paving the way to order of magnitude brighter cold molecular beams.},
archivePrefix = {arXiv},
arxivId = {1607.04844},
author = {Segev, Yair and Bibelnik, Natan and Akerman, Nitzan and Shagam, Yuval and Luski, Alon and Karpov, Michael and Narevicius, Julia and Narevicius, Edvardas},
doi = {10.1126/sciadv.1602258},
eprint = {1607.04844},
file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Segev et al. - 2017 - Molecular beam brightening by shock-wave suppression.pdf:pdf},
issn = {2375-2548},
journal = {Science Advances},
month = {mar},
number = {3},
pages = {e1602258},
title = {{Molecular beam brightening by shock-wave suppression}},
url = {http://arxiv.org/abs/1607.04844 http://advances.sciencemag.org/lookup/doi/10.1126/sciadv.1602258},
volume = {3},
year = {2017}
}
@article{Liu2017a,
abstract = {We have demonstrated that a supersonic beam of methyl radicals (CH3) in the ground rotational state of both para and ortho species has been slowed down to a standstill with a magnetic molecular decelerator, and successfully captured spatially in an anti-Helmholtz magnetic trap for {\textgreater} 1 s. The translational temperature of the trapped CH3 radicals was about 200 mK. The methyl radical is a non-polar polyatomic molecule, which is predicted to be an ideal system for further cooling below 1 mK via sympathetic cooling with ultracold atoms. In addition, it is a highly reactive intermediate that plays an important role in various processes in cold environments such as planetary atmospheres and the interstellar medium. The demonstrated trapping capability of methyl radicals opens up various possibilities for realizing ultracold ensembles of molecules towards Bose-Einstein condensation of polyatomic molecules and investigations of reactions governed by quantum statistics.},
archivePrefix = {arXiv},
arxivId = {1609.07735},
author = {Liu, Yang and Vashishta, Manish and Djuricanin, Pavle and Zhou, Sida and Zhong, Wei and Mittertreiner, Tony and Carty, David and Momose, Takamasa},
doi = {10.1103/PhysRevLett.118.093201},
eprint = {1609.07735},
file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Liu et al. - 2017 - Magnetic Trapping of Cold Methyl Radicals.pdf:pdf},
issn = {0031-9007},
journal = {Physical Review Letters},
month = {feb},
number = {9},
pages = {093201},
title = {{Magnetic Trapping of Cold Methyl Radicals}},
url = {http://arxiv.org/abs/1609.07735 https://link.aps.org/doi/10.1103/PhysRevLett.118.093201},
volume = {118},
year = {2017}
}
