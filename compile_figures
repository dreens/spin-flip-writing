# Export all the Figures from inkscape to PNG for latex to compile
# To run this, pipe it to bash: “cat compile_figures | bash”

inkscape -f $PWD/Geometry/Geometry_panels.svg -D -d 120 -e $PWD/geometry_out.png
inkscape -f $PWD/Blocking/figmakeblocking.svg -D -d 300 -e $PWD/blocking_out.png
inkscape -f $PWD/VWFig/tim-style-by-dave-1hz.svg -D -d 300 -e $PWD/vwfig_out.png
inkscape -f $PWD/LossSurfaces/Loss_Surfaces_Pins.svg -D -d 300 -e $PWD/losssurfaces_out.png
inkscape -f $PWD/MWSpec/Spec_prep_Inkscape.svg -D -d 300 -e $PWD/mwspec_out.png

# Adding Support for Supplementary Figures
inkscape -f $PWD/SuppFigs/Spectrum_FB/spectrum_fb.svg -D -d 300 -e $PWD/SuppFigs/specfb_out.png
inkscape -f $PWD/SuppFigs/E_Loss_Trajectories/elosstraj.svg -D -d 300 -e $PWD/SuppFigs/eil_out.png
inkscape -f $PWD/SuppFigs/E_Loss_Trajectories/beta_plot_fit_pretty.svg -D -d 300 -e $PWD/SuppFigs/beta_out.png
